<!--BREADCRUMB-->
<div id="container_breadcrumb">
    <div id="breadcrumb">
        <div class="col s12">
            <a href="/" class="breadcrumb">Principal</a>
            <a href="#" class="breadcrumb">Declaração de Residência</a>
        </div>
    </div>
</div>

<!--CONTAINER FORM-->
<form class="container center" id="container-master" action="" method="POST">

    <!--LOGO-->
    <div class="container center" id="logo_forms">
        <h5 class="left"><i><b id="name_form">Declaração de Residência<i
                        class="material-icons left small">assignment</i> </b></i>
    </div>
   

    <!--BUTTONS-->
    <div class="card lg " id="buttons">
        <a class="btn-medium  waves-effect waves-light btn" type="submit" href="#"><i class="material-icons left">save</i>Salvar</a>
        <a class="btn- center waves-effect waves-light btn"><i class="material-icons left">edit</i> Alterar</a>
        <a class="btn- center waves-effect waves-light btn"><i
                class="material-icons left">local_printshop</i>Imprimir</a>
        <a class="btn- center waves-effect waves-light btn"><i class="material-icons left">add_box</i>Importar</a>
    </div>


    <!--FORM-->
    <div id="container" class="container center card section">
        <div class="row">
            <form class="col">
                <div id="container_02" class="row">
                    <div class="input-field col s5">
                        <input autofocus name="nome" type="text" class="validate" required="">
                        <label for="nome">Nome:</label>
                    </div>
                    <div class="input-field col s2">
                        <input name="dateNascimento" type="date">
                        <label for="dateNascimento">Nascimento:</label>
                    </div>
                    <div class="input-field col s2">
                        <input name="nacionalidade" type="text">
                        <label for="nacionalidade">Nacionalidade:</label>
                    </div>
                    <div class="input-field col s2">
                        <input name="naturalidade" type="text">
                        <label for="naturalidade">Naturalidade:</label>
                    </div>
                    <div class="input-field col s1">
                        <input name="uf_nascimento" type="text">
                        <label for="uf_nascimento">UF</label>
                    </div>
                    <div class="input-field col s2">
                        <input placeholder="Digite o CEP" name="cep" type="text" id="cep">
                        <label for="cep">CEP:</label>
                    </div>
                    <div class="input-field col s5">
                        <input placeholder="Digite o endereço" type="text" id="logradouro">
                        <label for="first_name">Endereco:</label>
                    </div>
                    <div class="input-field col s2">
                        <input placeholder="Digite o numero" name="endereco" type="text">
                        <label for="endereco">Numero</label>
                    </div>
                    <div class="input-field col s2">
                        <input placeholder="Casa/Apt" name="complemento" type="text">
                        <label for="complemento">Complemento</label>
                    </div>
                    <div class="input-field col s4">
                        <input placeholder="Digite o bairro" name="bairro" type="text" id="bairro">
                        <label for="bairro">Bairro</label>
                    </div>
                    <div class="input-field col s4">
                        <input placeholder="Digite a cidade" name="cidade" type="text" id="localidade">
                        <label for="cidade">Cidade</label>
                    </div>
                    <div class="input-field col s1">
                        <input placeholder="Ex: SP" name="uf_endereco" type="text" id="uf">
                        <label for="uf_endereco">UF</label>
                    </div>
                    <div class="input-field col s2">
                        <input placeholder="CPF/CNPJ" name="cpf-cnpj" type="text">
                        <label for="cpf-cnpj">CPF/CNPJ</label>
                    </div>
                    <div class="input-field col s2">
                        <input placeholder="(000) 0000-0000" name="tel" type="text">
                        <label for="tel">TELEFONE</label>
                    </div>
                    <div class="input-field col s2">
                        <input placeholder="(000) 0000-0000" name="cel" type="text">
                        <label for="cel">CELULAR</label>
                    </div>
                    <div class="input-field col s3">
                        <input placeholder="Digite o email" name="email" type="text">
                        <label for="email">Email:</label>
                    </div>
                </div>
        </div>
    </div>

</form>